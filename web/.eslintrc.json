{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
   
  ],
  "parserOptions": {
    "ecmaFeatures": {
        "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "import/prefer-default-export": "off"
  },
  "ignorePatterns": [
    "dist"
  ]
}
